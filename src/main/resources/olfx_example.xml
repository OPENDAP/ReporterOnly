<?xml version="1.0" encoding="UTF-8"?>

<OLFSConfig>

    <LogReporter>
        <HyraxLogfilePath>/etc/olfs/logs/ExampleAnonymous.log</HyraxLogfilePath>
        
        <DefaultPing>
            <!-- 86400 - daily 60*60*24 -->
            600 <!-- every 10 mins -->
        </DefaultPing> 
        
        <CollectorUrl>
        	34.226.249.132:8080/collector/harvester/registration?
        </CollectorUrl>
        <ServerUrl>
        	http://test.opendap.org/opendap
        </ServerUrl>
        <ReporterUrl>
        	http://54.89.183.196:8080/reporter
        </ReporterUrl>
        <LogNumber>
        	100
        </LogNumber>
        
        <!--  LogFilePatternPath>
            logLinePattern.json
        </LogFilePatternPath -->
        
        <LogFilePattern>
            <names>host;sessionId;localDateTime;duration;httpStatus;requestId;httpVerb;resourceId;query;size</names>
            <regexp>\[(.*)\] \[(.*)\] \[(.*)\] \[(.*)\] \[(.*)\] \[(.*)\] \[(.*)\] \[(.*)\] \[(.*)\] \[(.*)\]</regexp>
        </LogFilePattern>
    </LogReporter>

</OLFSConfig>

