<?xml version="1.0" encoding="UTF-8"?>

<OLFSConfig>

    <LogReporter>
        <HyraxLogfilePath>Anonymous.log</HyraxLogfilePath>
        
        <DefaultPing>
            86400 <!-- daily 60*60*24-->
        </DefaultPing> 
        
        <CollectorUrl>
        	192.168.0.129:8081/harvester/registration?
        </CollectorUrl>
        <ServerUrl>
        	http://test.opendap.org/opendap
        </ServerUrl>
        <ReporterUrl>
        	http://34.226.249.132:8080/reporter
        </ReporterUrl>
        <LogNumber>
        	100
        </LogNumber>
        
        <!--  LogFilePatternPath>
            logLinePattern.json
        </LogFilePatternPath -->
        
        <LogFilePattern>
            <names>host;sessionId;localDateTime;duration;httpStatus;requestId;httpVerb;resourceId;query</names>
            <regexp>\[(.*)\] \[(.*)\] \[(.*)\] \[(.*)\] \[(.*)\] \[(.*)\] \[(.*)\] \[(.*)\] \[(.*)\]</regexp>
        </LogFilePattern>
    </LogReporter>

</OLFSConfig>

